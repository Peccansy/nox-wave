  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch server for debug",
      "runtimeVersion": "12.13.0",
      "runtimeExecutable": "${workspaceFolder}/.yarn/releases/yarn-berry.js",
      "runtimeArgs": ["node"],
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}